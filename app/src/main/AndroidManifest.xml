<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alancasas.intentsexamples">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_myicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.WebActivity" android:parentActivityName=".Activities.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".Activities.MainActivity">
            </meta-data>
        </activity>
        <activity android:name=".Activities.ContactsIntent" android:parentActivityName=".Activities.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".Activities.MainActivity">
            </meta-data>

        </activity>
        <activity android:name=".Activities.EmailActivity" android:parentActivityName=".Activities.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".Activities.MainActivity">
            </meta-data>

        </activity>
        <activity android:name=".Activities.PhoneActivity" android:parentActivityName=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".Activities.MainActivity">
            </meta-data>

        </activity>
        <activity android:name=".Activities.CameraActivity" android:parentActivityName=".Activities.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".Activities.MainActivity">
            </meta-data>

        </activity>
        <activity android:name=".Activities.MainActivity">
        </activity>
    </application>

</manifest>